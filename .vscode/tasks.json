{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++-11.2.0 build active file",
			"command": "/usr/bin/g++-11",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-std=c++20",
				"${env:HOME}/workspace/cpp/36/*.cpp",
				"-o",
				"${env:HOME}/workspace/cpp/36/rooster"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/g++-11"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: clang++ 14.0.0 build active file",
			"command": "/usr/bin/clang++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-std=c++20",
				"${env:HOME}/workspace/cpp/36/*.cpp",
				"-o",
				"${env:HOME}/workspace/cpp/36/rooster"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/clang++"
		}
	]
}